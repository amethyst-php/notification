{
    "name": "amethyst/notification",
    "keywords": ["laravel", "amethyst", "amethyst-notification", "notification"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "amethyst/core": "0.3.*",
        "amethyst/user": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\NotificationServiceProvider"
            ]
        }
    }
}
